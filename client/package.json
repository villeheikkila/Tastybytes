{
  "name": "tastekeeper",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate": "npm run generate:barrels && npm run generate:hooks",
    "generate:barrels": "barrelsby --directory ./src --location replace --delete",
    "generate:hooks": "graphql-codegen --config codegen.yml",
    "schemas:watch": "npm run schemas -- --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "@hookform/error-message": "0.0.5",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-google-recaptcha": "^2.1.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.5",
    "apollo-upload-client": "^14.1.3",
    "framer-motion": "^3.0.0",
    "fsevents": "^2.2.1",
    "graphql": "^15.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.2.4",
    "react-google-recaptcha": "^2.1.0",
    "react-hook-form": "^6.13.1",
    "react-intersection-observer-hook": "^1.0.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "styled-components": "^5.2.1",
    "subscriptions-transport-ws": "^0.9.18",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.19.4",
    "@graphql-codegen/import-types-preset": "^1.18.1",
    "@graphql-codegen/near-operation-file-preset": "^1.17.13",
    "@graphql-codegen/typescript": "^1.19.0",
    "@graphql-codegen/typescript-operations": "^1.17.12",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "barrelsby": "^2.2.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">5%"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "fsevents": "^2.2.1"
  }
}
