{
  "private": true,
  "name": "tasted",
  "description": "",
  "license": "",
  "scripts": {
    "build": "remix build",
    "dev": "remix watch",
    "postinstall": "remix setup node",
    "start": "NODE_ENV=production ts-node-dev server/index.ts",
    "start:dev": "NODE_ENV=development ts-node-dev --respawn server/index.ts",
    "codegen": "graphql-codegen --watch --config codegen.yml",
    "watch": "concurrently \"npm run dev\" \"npm run start:dev \" \"npm run codegen \"",
    "gm": "NODE_OPTIONS=\"$NODE_OPTIONS -r ./scripts/source-env.js\" graphile-migrate",
    "gmp": "NODE_ENV=production NODE_OPTIONS=\"$NODE_OPTIONS -r ./scripts/source-env.js\" graphile-migrate",
    "reset": "ts-node ./scripts/database-create.ts",
    "deploy": "fly deploy --remote-only"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@graphile-contrib/pg-simplify-inflector": "^6.1.0",
    "@remix-run/express": "^1.1.1",
    "@remix-run/react": "^1.1.1",
    "@stitches/react": "^1.2.6",
    "compression": "^1.7.4",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^14.2.0",
    "express": "^4.17.1",
    "graphile-migrate": "^1.2.0",
    "graphql-request": "^3.7.0",
    "graphql-tag": "^2.12.6",
    "morgan": "^1.10.0",
    "postgraphile": "^4.12.8",
    "postgraphile-plugin-connection-filter": "^2.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.3.1",
    "@graphql-codegen/typed-document-node": "^2.2.2",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-graphql-request": "^4.3.3",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@remix-run/dev": "^1.1.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
