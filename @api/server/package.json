{
  "name": "@api/server",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "tsc -b",
    "start": "node -r @api/config/env dist/index.js",
    "dev": "nodemon --signal SIGINT --watch 'dist/**/*.js' -x \"node --inspect=9678 -r @api/config/env -r source-map-support/register\" dist/index.js",
    "schema:export": "NODE_OPTIONS=\"$NODE_OPTIONS -r @api/config/env\" ts-node scripts/schema-export.ts",
    "test": "NODE_ENV=test NODE_OPTIONS=\"$NODE_OPTIONS -r @api/config/env\" jest",
    "gw": "cd dist && NODE_OPTIONS=\"$NODE_OPTIONS -r @api/config/env\" graphile-worker --crontab ../crontab",
    "install-db-schema": "mkdirp dist && yarn gw --schema-only"
  },
  "dependencies": {
    "@app/client": "0.0.0",
    "@api/config": "0.0.0",
    "@graphile-contrib/constraint-error-tags": "^1.0.3",
    "@graphile-contrib/pg-many-to-many": "^1.0.1",
    "@graphile-contrib/pg-simplify-inflector": "^6.1.0",
    "@graphile/pg-pubsub": "^4.12.1",
    "@graphile/pro": "^1.0.3",
    "@types/connect-pg-simple": "^4.2.0",
    "@types/csurf": "^1.11.2",
    "@types/express-session": "^1.17.0",
    "@types/helmet": "^4.0.0",
    "@types/html-to-text": "^8.0.1",
    "@types/lodash": "^4.14.175",
    "@types/mjml": "^4.7.0",
    "@types/morgan": "^1.9.3",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.7",
    "@types/passport-github2": "^1.2.5",
    "@types/pg": "^8.6.1",
    "aws-sdk": "^2.1009.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.2",
    "connect-pg-simple": "^6.2.1",
    "csurf": "^1.11.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "graphile-build": "^4.12.0",
    "graphile-build-pg": "^4.12.1",
    "graphile-utils": "^4.12.1",
    "graphile-worker": "^0.11.4",
    "helmet": "^4.6.0",
    "html-to-text": "^8.0.0",
    "lodash": "^4.17.21",
    "mjml": "^4.10.4",
    "morgan": "^1.10.0",
    "next": "^11.1.2",
    "nodemailer": "^6.7.0",
    "passport": "^0.5.0",
    "passport-github2": "^0.1.12",
    "pg": "^8.7.1",
    "postgraphile": "^4.12.4",
    "source-map-support": "^0.5.20",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "^15.6.1",
    "graphql": "^15.6.1",
    "jest": "^27.2.5",
    "mkdirp": "^1.0.4",
    "mock-req": "^0.2.0",
    "ts-node": "^10.3.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.5",
    "utf-8-validate": "^5.0.7"
  }
}
