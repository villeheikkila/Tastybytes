{
  "name": "tastekeeper",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node-dev --respawn src/index.ts",
    "build": "tsc -p . --outDir build",
    "lint": "prettier --write . && eslint . --ext .ts",
    "generate": "graphql-codegen --config codegen.yml",
    "db:migrate": "ts-node node_modules/.bin/typeorm migration:run",
    "seed:generate": "ts-node node_modules/.bin/typeorm migration:generate -c seed -n ",
    "seed:run": "ts-node node_modules/.bin/typeorm migration:run -c seed",
    "seed:revert": "cd src && ts-node node_modules/.bin/typeorm migration:revert -c seed"
  },
